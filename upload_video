import boto3

def upload_video(video_path, metadata):
    # Create an S3 client
    s3_client = boto3.client('s3')

    # Bucket and file details
    bucket_name = 'unverifiedimages'
    file_key = 'video.mp4'  # The desired key in the S3 bucket

    try:
        # Open the video file in binary mode and upload it
        with open(video_path, 'rb') as video:
            response = s3_client.put_object(Bucket=bucket_name, Key=file_key, Body=video, Metadata=metadata)
            print(f"Upload successful. Response: {response}")
    except Exception as e:
        print(f'We had an exception: {e}')

metadata = {"Camera Number": "1", "Time": "18:19:38", "Location": "42.39379516666666, -72.52867866666666", "Signature_Base64": ""}
upload_video("./output.mp4",metadata)